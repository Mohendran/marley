{
  "name": "marley",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "engines": {
    "node": "8.1.4",
    "npm": "5.0.3"
  },
  "scripts": {
    "ng": "ng",
    "lint": "tslint . --config ./config/tslint.json",
    "test": "ng test",
    "e2e": "ng e2e",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "webpack-dashboard": "webpack-dashboard -- node app.js",
    "analyzer": "npm run build:prod && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js --config ./tools/analyzer.js",
    "rollup": "rollup --config ./config/inline.config.js && rollup --config ./config/main.config.js && rollup --config ./config/styles.config.js && rollup --config ./config/vendor.config.js && rollup --config ./config/polyfills.config.js",
    "build": "ng build --env=prod --build-optimizer=true",
    "build:dev": "ng build",
    "build:prod": "npm run build && npm run rollup",
    "build:desktop": "node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js --config ./config/desktop.config.js",
    "package": "electron-packager ./desktop \"Spaceti Superadmin\" --platform=darwin --arch=x64 --version=2.0.0 --prune=true --app-copyright=\"Copyright (c) Spaceti.\" --icon=assets/icon.png --out=./desktop/releases/mac --overwrite",
    "package:linux": "electron-packager ./desktop \"Spaceti Superadmin\" --platform=linux --arch=x64 --version=2.0.0 --prune=true --app-copyright=\"Copyright (c) Spaceti.\" --icon=assets/icon.png --out=./desktop/releases/linux --overwrite",
    "package:windows": "electron-packager ./desktop \"Spaceti Superadmin\" --platform=win32 --arch=ia32 --version=2.0.0 --prune=true --app-copyright=\"Copyright (c) Spaceti.\" --icon=assets/icon.png --out=./desktop/releases/windows --overwrite --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Spaceti Superadmin\"",
    "package:windows:x64": "electron-packager ./desktop \"Spaceti Superadmin\" --platform=win32 --arch=x64 --version=2.0.0 --prune=true --app-copyright=\"Copyright (c) Spaceti.\" --icon=assets/icon.png --out=./desktop/releases/windows --overwrite --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Spaceti Superadmin\"",
    "serve": "ng serve",
    "serve:dev": "npm run build:dev && npm start",
    "serve:prod": "npm run build:prod && npm start",
    "serve:electron": "npm run build:prod && npm run build:desktop && NODE_ENV=development electron main",
    "start": "node ./server/app.js"
  },
