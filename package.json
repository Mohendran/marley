{
    "name": "marley",
    "version": "0.3.4",
    "license": "MIT",
    "angular-cli": {},
    "engines": {
        "node": "6.9.2",
        "npm": "3.10.9"
    },
    "scripts": {
        "ng": "ng",
        "prefixcss": "postcss -s postcss-scss -u autoprefixer --autoprefixer.browsers \"last 2 versions\" -r src/**/*.scss",
        "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor",
        "build": "npm run prefixcss && ng build --aot && rollup --config ./config/rollup-inline.config.js && rollup --config ./config/rollup-main.config.js && rollup --config ./config/rollup-styles.config.js && rollup --config ./config/rollup-vendor.config.js",
        "start": "node server.js"
    },
    "dependencies": {
        "@angular/common": "^4.0.2",
        "@angular/compiler": "^4.0.2",
        "@angular/core": "^4.0.2",
        "@angular/forms": "^4.0.2",
        "@angular/http": "^4.0.2",
        "@angular/platform-browser": "^4.0.2",
        "@angular/platform-browser-dynamic": "^4.0.2",
        "@angular/router": "^3.3.1",
        "bulma": "^0.3.1",
        "core-js": "^2.4.1",
        "koa": "^2.3.0",
        "koa-bodyparser": "^4.2.0",
        "koa-router": "^7.2.1",
        "koa-static": "^4.0.1",
        "rxjs": "^5.0.1",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.7.2"
    },
    "devDependencies": {
        "@angular/cli": "^1.1.3",
        "@angular/compiler-cli": "^4.0.2",
        "@types/jasmine": "2.5.38",
        "@types/node": "^6.0.42",
        "autoprefixer": "^7.1.2",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "codelyzer": "~2.0.0-beta.1",
        "jasmine-core": "2.5.2",
        "jasmine-spec-reporter": "2.5.0",
        "karma": "1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.2.1",
        "postcss": "^6.0.9",
        "postcss-cli": "^4.1.0",
        "postcss-scss": "^1.0.2",
        "protractor": "~4.0.13",
        "rollup": "^0.43.0",
        "rollup-plugin-babel": "^2.7.1",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-multi-entry": "^2.0.1",
        "rollup-plugin-node-globals": "^1.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "ts-node": "1.2.1",
        "tslint": "^4.3.0",
        "typescript": "~2.0.3"
    }
}
